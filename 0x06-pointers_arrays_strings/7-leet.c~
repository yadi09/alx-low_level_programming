#include "main.h"
/**
 * leet - function
 * this function encodes a string into 1337.
 * @str: parameter
 * Return: 0;
 */
char *leet(char *str)
{
int i = 0, j = 0;
char string[] = "AaEeOoTtLl";
char encode[] = "4433007711";

while (*(str + i) != '\0')
{
while (j < 10)
{
if (string[j] == str[i])
{
str[i] = encode[j];
}
j++;
}
i++;
}

return (str);
}
